log("Hello World!")

// Comment

// Variable
a = 1

// conditional
if(a == 1) {
    log("a is 1")
} elseif(a == 2) {
    log("a is 2")
} else {
    log("a is not 1 or 2")
}

// loop
while (a < 10) {
    a++
}

// function
functionName() {
    // code
}

// function call
functionName()

// Boolean order of ops
a = 0
b = 0
c = 1
d = (a && b) || c
e = a && (b || c)

// escaping
c = "Hello \" World!"
